<wpf:View x:Class="NavigateSharp.Example.Wpf.Presenters.Welcome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:presentation="clr-namespace:NavigateSharp.Example.Wpf.Presentation"
        xmlns:wpf="clr-namespace:NavigateSharp.Wpf;assembly=NavigateSharp.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <TextBlock Name="welcomeText"/>
        <Button Grid.Row="1" Name="welcomeOkButton" Click="WelcomeOk_Click" Content="Ok"/>
    </Grid>
</wpf:View>